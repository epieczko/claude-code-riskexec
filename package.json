{
  "name": "claude-code-riskexec",
  "version": "1.21.1",
  "description": "Component templates and tracking system for Claude Code",
  "main": "cli-tool/src/index.js",
  "bin": {
    "claude-code-templates": "cli-tool/bin/create-claude-config.js",
    "cct": "cli-tool/bin/create-claude-config.js"
  },
  "scripts": {
    "dev": "vercel dev",
    "build": "echo 'Build complete'",
    "start": "vercel dev",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\" \"tests/**/*.{ts,tsx,js,jsx}\" \"scripts/**/*.{ts,js}\"",
    "agent:security-auditor": "security-auditor 'Security audit covering codebase, dependencies, and deployment configuration'",
    "policy:models": "node scripts/model-policy-check.js",
    "policy:router": "node scripts/router-verify.js",
    "policy:all": "npm run policy:models && npm run policy:router",
    "router:install": "node ./scripts/router-install.js",
    "router:verify": "node ./scripts/router-verify.js",
    "router:code": "npx @musistudio/claude-code-router code",
    "router:ui": "npx @musistudio/claude-code-router ui",
    "components:validate": "node scripts/validateBmadComponents.js",
    "phase-docs": "ts-node scripts/generatePhaseDocs.ts",
    "workflow:verify": "node scripts/verifyWorkflowConfig.js",
    "agentos:command-map": "ts-node --project tsconfig.scripts.json scripts/generateAgentOsCommandMap.ts",
    "agentos:verify": "ts-node --project tsconfig.scripts.json scripts/verifyAgentOSIntegration.ts",
    "format": "prettier -w .",
    "spec-kit:workflow": "node scripts/runSpecKitWorkflow.js",
    "spec-workflow": "node -r ts-node/register src/workflowOrchestrator.ts"
  },
  "dependencies": {
    "@vercel/postgres": "^0.10.0",
    "boxen": "^5.1.2",
    "chalk": "^5.6.2",
    "chokidar": "^3.5.3",
    "claude-code-router": "^2.1.1",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.6",
    "open": "^8.4.2",
    "ora": "^5.4.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@musistudio/claude-code-router": "^1.0.49",
    "@types/jest": "^30.0.0",
    "@vercel/node": "^3.0.0",
    "front-matter": "^4.0.2",
    "jest": "^30.2.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "yaml": "^2.8.1"
  },
  "keywords": [
    "claude-code",
    "templates",
    "components",
    "tracking",
    "analytics"
  ],
  "author": "Claude Code RiskExec"
}
